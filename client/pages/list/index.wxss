page{
  
}
.none{
  display: none;
}
.filter{
  display: inline-block;
  width:100%;
  background-color: #fff;
}
.filter>view{
  line-height: 40px;
  text-align: center;
  display: inline-block;
  width:25%;
  box-sizing: border-box;
}
.filter>view.active{
  color: #F26166;
}
.filter .iconfont{
  display:inline;
}
.filter1{
  font-size:14px;
  position: absolute;
  background: #fff;
  width:100%;
  border-top: 1px solid #F0F0F0;
  padding: 10px;
}
.filter1 checkbox-group label{
  margin-right:6px;
}

.filter1 input{
  display: inline-block;
  width: 40px;
  height: 24px;
  border-radius: 2px;
  border: 1px solid #e6e6e6;
  vertical-align: middle;
}

.filter1 .price-filter{
  margin-top: 10px;
}
.filter1 .ok{
  display: inline;
  color: #fff;
  background-color: #f50;
  padding: 3px 10px;
  border-radius: 4px;
  vertical-align: middle;
  margin-left: 10px;
}

.list{

}
.list .item:nth-of-type(odd){
  padding-right: 0;
}
.list .item{
  display: inline-block;
  width:50%;
  overflow: hidden;
  padding: 6px;
  box-sizing: border-box;
}
.list .item image {
  width:100%;
  height:160px;
}
.list .item .title{
  white-space:nowrap;
  overflow:hidden; 
  text-overflow:ellipsis;
}
.list .item .price{
  display: inline;
  color: #F26166;
}
.list .item .price .y{
  font-size: 12px;
  margin-top: 4px;
}
.list .item .ys{
  color: #ccc;
  font-size: 12px;
  line-height: 22px;
  margin-top: 2px;
}