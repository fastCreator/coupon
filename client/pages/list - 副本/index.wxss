page{
  height:100%;
  overflow: hidden;
  padding: 0;
}
scroll-view{
  height:100%;
}
.none{
  display: none;
}
.filter{
  display: inline-block;
  width:100%;
  background-color: #fff;
}
.filter>view{
  line-height: 40px;
  text-align: center;
  display: inline-block;
  width:25%;
  box-sizing: border-box;
}
.filter>view.active{
  color: #F26166;
}
.filter .iconfont{
  display:inline;
}

.list .item:nth-of-type(odd){
  padding-right: 0;
}
.list .item{
  display: inline-block;
  width:50%;
  overflow: hidden;
  padding: 6px;
  box-sizing: border-box;
}
.list .item image {
  width:100%;
  height:160px;
}
.list .item .title{
  white-space:nowrap;
  overflow:hidden; 
  text-overflow:ellipsis;
  font-size:12px;
} 
.list .item .coupon{
  background: #F26166;
  color: #fff;
  border: 1px solid #F26166;
  border-radius: 5px;
  font-size: 12px;
  line-height: 12px;
  padding: 2px;
}
.list .item .endprice{
  font-size:12px;
  color: #F26166;
}