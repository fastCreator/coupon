page, scroll-view {
  height: 100%;
}
image{
  height:100px;
}
.list .item {
  width: 33.33333%;
  display: inline-block;
  text-align: center;
  border-bottom: 1px solid #ededed;
  border-right: 1px solid #ededed;
  padding: 4px;
  box-sizing: border-box;
}

.item .title {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 12px;
}

.weui-search-bar__cancel-btn {
  display: inline;
  font-size: 14px;
  float: right;
}
.out-title{
  background: #F3F3F3;
  padding: 4px 8px;
}
.goH{
  position: fixed;
  right: 8px;
  top:50%;
  font-size: 30px;
  background: #FBEF05;
  border-radius: 100%;
  display: inline-block;
  width:1.5em;
  height:1.5em;
  line-height: 1.5em;
  text-align: center;
  color: #E41E51;
}